{% assign unique_tags = '' | split: ',' %}
{% assign tags = site.links | map: 'tags' | join: ',' | join: ',' | split: ',' %}
{% assign tags = tags | sort %}
{% for tag in tags %}
  {% unless tag == previous %}
    {% assign unique_tags = unique_tags | push: tag %}
  {% endunless %}
  {% assign previous = tag %}
{% endfor %}
